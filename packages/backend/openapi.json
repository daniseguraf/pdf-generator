          },
          "district": {
            "type": "string",
            "example": "Main District",
            "description": "Building district"
          },
          "city": {
            "type": "string",
            "example": "Main City",
            "description": "Building city"
          },
          "province": {
            "type": "string",
            "example": "Main Province",
            "description": "Building province"
          },
          "postalCode": {
            "type": "string",
            "example": "15001",
            "description": "Building postal code"
          },
          "floors": {
            "type": "number",
            "example": 10,
