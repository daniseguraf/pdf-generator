      "RegisterUserDto": {
        "type": "object",
        "properties": {
          "firstName": {
            "type": "string",
            "example": "John Doe",
            "description": "User first name"
          },
          "lastName": {
            "type": "string",
            "example": "Doe",
            "description": "User last name"
          },
          "email": {
            "type": "string",
            "example": "john.doe@example.com",
            "description": "User email"
          },
          "password": {
            "type": "string",
            "example": "password",
            "description": "User password"
          }
        },
