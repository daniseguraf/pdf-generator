  "info": {
    "title": "My Buildings API",
    "description": "Building Management API",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "schemas": {
      "CreateBuildingDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Main Building",
            "description": "Building name"
          },
          "description": {
            "type": "string",
            "example": "New building in the city",
            "description": "Building description"
          },
          "yearBuilt": {
            "type": "number",
            "example": 2020,
            "description": "Year built"
          },
          "propertyType": {
            "type": "string",
            "example": "RESIDENTIAL",
            "description": "Property type",
            "enum": [
              "RESIDENTIAL",
              "COMMERCIAL",
              "MIXED"
            ]
          },
          "address": {
            "type": "string",
            "example": "Main Street",
            "description": "Building address"
          },
          "district": {
            "type": "string",
            "example": "Main District",
            "description": "Building district"
          },
          "city": {
            "type": "string",
            "example": "Main City",
            "description": "Building city"
          },
          "province": {
            "type": "string",
            "example": "Main Province",
            "description": "Building province"
          },
          "postalCode": {
            "type": "string",
            "example": "15001",
            "description": "Building postal code"
          },
          "floors": {
            "type": "number",
            "example": 10,
            "description": "Number of floors"
          },
          "phoneNumber": {
            "type": "string",
            "example": "+51 987 654 321",
            "description": "Building phone number"
          },
          "email": {
            "type": "string",
            "example": "contact@building.com",
            "description": "Building email"
          },
          "amenities": {
            "type": "array",
            "example": [
              "ELEVATOR",
              "PARKING"
            ],
            "description": "Building amenities",
            "items": {
              "type": "string",
              "enum": [
                "PARKING",
                "SECURITY_24_7",
                "ELEVATOR",
                "WHEELCHAIR_ACCESS",
                "WHEELCHAIR_LIFT",
                "FIRE_ALARM",
                "CAMERAS"
              ]
            }
          }
        },
        "required": [
          "name",
          "yearBuilt",
          "propertyType",
          "address",
          "district",
          "city",
          "province",
          "floors",
          "amenities"
        ]
      },
      "Building": {
        "type": "object",
        "properties": {}
      },
      "UpdateBuildingDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Main Building",
            "description": "Building name"
          },
          "description": {
            "type": "string",
            "example": "New building in the city",
            "description": "Building description"
          },
          "yearBuilt": {
            "type": "number",
            "example": 2020,
            "description": "Year built"
          },
          "propertyType": {
            "type": "string",
            "example": "RESIDENTIAL",
            "description": "Property type",
            "enum": [
              "RESIDENTIAL",
              "COMMERCIAL",
              "MIXED"
            ]
          },
          "address": {
            "type": "string",
            "example": "Main Street",
            "description": "Building address"
          },
          "district": {
            "type": "string",
            "example": "Main District",
            "description": "Building district"
          },
          "city": {
            "type": "string",
            "example": "Main City",
            "description": "Building city"
          },
          "province": {
            "type": "string",
            "example": "Main Province",
            "description": "Building province"
          },
          "postalCode": {
            "type": "string",
            "example": "15001",
            "description": "Building postal code"
          },
          "floors": {
            "type": "number",
            "example": 10,
            "description": "Number of floors"
          },
          "phoneNumber": {
            "type": "string",
            "example": "+51 987 654 321",
            "description": "Building phone number"
          },
          "email": {
            "type": "string",
            "example": "contact@building.com",
            "description": "Building email"
          },
          "amenities": {
            "type": "array",
            "example": [
              "ELEVATOR",
              "PARKING"
            ],
            "description": "Building amenities",
            "items": {
              "type": "string",
              "enum": [
                "PARKING",
                "SECURITY_24_7",
                "ELEVATOR",
                "WHEELCHAIR_ACCESS",
                "WHEELCHAIR_LIFT",
                "FIRE_ALARM",
                "CAMERAS"
              ]
            }
          }
        }
      },
      "RegisterUserDto": {
        "type": "object",
        "properties": {
          "firstName": {
            "type": "string",
            "example": "John Doe",
            "description": "User first name"
          },
          "lastName": {
            "type": "string",
            "example": "Doe",
            "description": "User last name"
          },
          "email": {
            "type": "string",
            "example": "john.doe@example.com",
            "description": "User email"
          },
          "password": {
            "type": "string",
            "example": "password",
            "description": "User password"
          }
        },
        "required": [
          "firstName",
          "lastName",
          "email",
          "password"
        ]
      },
      "AuthResponse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "description": "User ID",
            "example": 1
          },
          "firstName": {
            "type": "string",
            "description": "User first name",
            "example": "John"
          },
          "lastName": {
            "type": "string",
            "description": "User last name",
            "example": "Doe"
          },
          "email": {
            "type": "string",
            "description": "User email",
            "example": "john.doe@example.com"
          },
          "accessToken": {
            "type": "string",
            "description": "JWT access token",
            "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
          }
        },
        "required": [
          "id",
          "firstName",
          "lastName",
          "email",
          "accessToken"
        ]
      },
      "LoginUserDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "john.doe@example.com",
            "description": "User email"
          },
          "password": {
            "type": "string",
            "example": "password",
            "description": "User password"
          }
        },
        "required": [
          "email",
          "password"
        ]
      }
    }
  }
}